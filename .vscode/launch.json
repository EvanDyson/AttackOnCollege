  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng serve",
      "type": "pwa-chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/"
    },
    {
      "name": "ng test",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    },
    {
      "name": "Go Debugger",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}\\back_end\\src",
      "env": {"GITHUB_TOKEN": "xxxxx"}
    }
  ]
}
